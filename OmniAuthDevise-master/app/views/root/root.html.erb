<% if user_signed_in? %>
<%= current_user.to_json %>

<br />
<a href="<%= destroy_user_session_url %>" class="btn btn-primary">Logout</a>

<%#= current_user.twitter_client.home_timeline.first.to_json %>

<% else %>
<%= link_to "Sign Up", new_user_registration_url, class: "btn btn-primary" %>
<%= link_to "Sign In", user_session_url, class: "btn btn-primary" %>
